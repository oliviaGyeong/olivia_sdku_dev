<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/sub_bg"
    android:orientation="vertical" >

    <ScrollView
        android:id="@+id/sv_catetc"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fadeScrollbars="true">
        <LinearLayout 
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">
            <fragment
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:id="@+id/fragmentCon"
		        android:name="com.example.kcpsecutest.common.ConmodeLayout"
		        tools:layout="@layout/activity_main" />
            <GridLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:columnCount="4"
            android:rowCount="3" >
				<TextView 
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:text="메세지1" />
			    <EditText 
			        android:id="@+id/edit_reqmsg1_catmember"
			        android:hint="안녕하세요"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content" />
			    <TextView 
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:text="메세지2" />
			    <EditText 
			        android:id="@+id/edit_reqmsg2_catmember"
			        android:hint="KCP입니다."
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content" />
			    <TextView 
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:text="메세지3" />
			    <EditText 
			        android:id="@+id/edit_reqmsg3_catmember"
			        android:hint="이용해주셔서"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content" />
			    <TextView 
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:text="메세지4" />
			    <EditText 
			        android:id="@+id/edit_reqmsg4_catmember"
			        android:hint="감사합니다."
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content" />
			    <TextView 
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:text="시작날자" />
			    <EditText 
			        android:id="@+id/edit_stdt_catmember"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content" />
			    <TextView 
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content"
			        android:text="종료날자" />
			    <EditText 
			        android:id="@+id/edit_endt_catmember"
			        android:layout_width="wrap_content"
			        android:layout_height="wrap_content" />
			</GridLayout>
            <GridLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:columnCount="2"
            android:rowCount="5" >
            
        	<Button
	            android:id="@+id/btn_storeinfo_catetc"
	            android:text="가맹점정보확인"
	            android:layout_gravity="center"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content" />
        	<Button
	            android:id="@+id/btn_lasttrade_catetc"
	            android:text="마지막거래확인"
	            android:layout_gravity="center"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content" />
        	<Button
	            android:id="@+id/btn_signdata_catetc"
	            android:text="서명데이터확인"
	            android:layout_gravity="center"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content" />
        	<Button
	            android:id="@+id/btn_reprint_catetc"
	            android:text="재인쇄"
	            android:layout_gravity="center"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content" />

        	<Button
        	    android:id="@+id/btn_cattot_catetc"
        	    android:layout_gravity="center"
        	    android:layout_width="wrap_content"
        	    android:layout_height="wrap_content"
        	    android:text="단말기집계" />

        	<Button
	            android:id="@+id/btn_hosttot_catetc"
	            android:text="호스트집계"
	            android:layout_gravity="center"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content" />
        	<Button
	            android:id="@+id/btn_signreq_catetc"
	            android:text="사인요청"
	            android:layout_gravity="center"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content" />
        	<Button
	            android:id="@+id/btn_msdata_catetc"
	            android:text="식별정보요청"
	            android:layout_gravity="center"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content" />
        	<Button
	            android:id="@+id/btn_passwd_catetc"
	            android:text="비밀번호요청"
	            android:layout_gravity="center"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content" />
        	<Button
	            android:id="@+id/btn_icput_catetc"
	            android:text="IC삽입여부"
	            android:layout_gravity="center"
	            android:layout_width="wrap_content"
	            android:layout_height="wrap_content" />
		</GridLayout>
        </LinearLayout>
        </ScrollView>


</LinearLayout>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@drawable/sub_bg">

   
    <LinearLayout
        android:orientation="horizontal"
        android:paddingBottom="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:text="사업자번호 : "
            android:gravity="right"
            android:layout_width="100dp"
            android:layout_height="wrap_content" />
		<EditText
            android:id="@+id/edit_busino_key"
            android:layout_width="151dp"
            android:layout_height="wrap_content"
            android:background="#FFFFFF"
            android:inputType="number"
            android:text="1138521083"
            android:singleLine="true" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:paddingBottom="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:text="public_key_ver : "
            android:gravity="right"
            android:layout_width="100dp"
            android:layout_height="wrap_content" />
		<EditText
            android:id="@+id/edit_public_key"
            android:layout_width="151dp"
            android:layout_height="wrap_content"
            android:background="#FFFFFF"
            android:inputType="number"
            android:text="00"
            android:singleLine="true" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:paddingBottom="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:text="input_enc : "
            android:gravity="right"
            android:layout_width="100dp"
            android:layout_height="wrap_content" />
		<EditText
            android:id="@+id/edit_input_enc"
            android:layout_width="151dp"
            android:layout_height="wrap_content"
            android:background="#FFFFFF"
            android:inputType="number"
            android:text=""
            android:singleLine="true" />
    </LinearLayout>
    <LinearLayout
        android:orientation="horizontal"
        android:paddingBottom="10dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <TextView
            android:text="ksn : "
            android:gravity="right"
            android:layout_width="100dp"
            android:layout_height="wrap_content" />
		<EditText
            android:id="@+id/edit_ksn"
            android:layout_width="151dp"
            android:layout_height="wrap_content"
            android:background="#FFFFFF"
            android:inputType="number"
            android:text="GFNGFQgAAjAwMg=="
            android:singleLine="true" />
    </LinearLayout>
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <Button
            android:id="@+id/btn_key_cert"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="요청" />

        <Button
            android:id="@+id/btn_key"
            android:layout_width="100dp"
            android:layout_height="wrap_content"
            android:text="완료" />
    </LinearLayout>

</LinearLayout>